            <kind>1</kind>
            <string>///Collisions!

//Down
if place_meeting(x,y,block_parent)
if vspeed &gt;= 0
    {
    vspeed=0;
    repeat(100)
        {
        y -= 1;
        if !place_meeting(x,y,block_parent) break;
        }
    }
//Up
if place_meeting(x,y,block_parent)
if vspeed &lt; 0
    {
    vspeed=0;
    repeat(100)
        {
        y += 1;
        if !place_meeting(x,y,block_parent) break;
        }
    }
//Right
if place_meeting(x+4,y,block_parent)
if hspeed &gt;= 0
    {
    hspeed=0;
    repeat(100)
        {
        x -= 1;
        if !place_meeting(x+4,y,block_parent) break;
        }
    }
//Left
if place_meeting(x-4,y,block_parent)
if hspeed &lt; 0
    {
    hspeed=0;
    repeat(100)
        {
        x += 1;
        if !place_meeting(x-4,y,block_parent) break;
        }
    }
//On Ground = 1
if place_meeting(x,y+4,block_parent)
onground = 1;
else onground = 0;

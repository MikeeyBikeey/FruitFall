            <string>///Walking

if place_meeting(x,y,obj_player2) onground=1;


if keyboard_check(ord('A'))
if hspeed &gt; -maxspd 
hspeed -= acl;

if keyboard_check(ord('D'))
if hspeed &lt; maxspd 
hspeed += acl;

//Jump
if onground == 1
    {
    gra_spd = 0;
    if keyboard_check_pressed(vk_space)
    vspeed -= jmp_str;
    }
else
    {
    if vspeed &lt; gra_spdy
        {
        vspeed += gra_spd;
        gra_spd += gra_str;
        }
    }

//frc
if hspeed &lt; -1 hspeed += frc
else if hspeed &gt; 1 hspeed -= frc
else hspeed = 0
if vspeed &lt; -1 vspeed += frc
else if vspeed &gt; 1 vspeed -= frc
else vspeed = 0;

if keyboard_check(ord('A'))
if keyboard_check(ord('D')) hspeed = 0; 
